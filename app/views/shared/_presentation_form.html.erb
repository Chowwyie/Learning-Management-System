<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_with model: @presentation do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>
      <%= f.collection_select(:users, studentList, :id, :name, {}, {:multiple => true}) %>
      <%# <input name="user_id" type="text"> %>
      <%= f.submit "Make New Presentation", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>